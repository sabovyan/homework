{
	"source": {
		"include": ["0-set", "1-map", "2-recursion"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/docs)"
	},
	"plugins": ["plugins/markdown"],
	"template": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/"
	}
}
